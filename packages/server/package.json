  "license": "WAGMIT",
  "version": "0.0.0",
  "repository": "tmm/wagmi",
  "author": "awkweb.eth",
  "ethereum": "awkweb.eth",
  "funding": [
    {
      "type": "gitcoin",
      "url": "https://gitcoin.co/grants/4493/wagmi-react-hooks-library-for-ethereum"
    },
    {
      "type": "github",
